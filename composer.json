{
    "name": "ilmala/archivium-api",
    "description": "Unofficial Php Archivium Api",
    "keywords": [
        "ilmala",
        "laravel",
        "archivium-api"
    ],
    "homepage": "https://github.com/ilmala/archivium-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca",
            "email": "luca.malattia@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "illuminate/http": "^8.56",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Ilmala\\Archivium\\": "src",
            "Ilmala\\Archivium\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilmala\\Archivium\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ilmala\\Archivium\\ArchiviumServiceProvider"
            ],
            "aliases": {
                "Archivium": "Ilmala\\Archivium\\ArchiviumFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
